extends layout
block content     
    h4.display-4.text-center #{message} 
    .row
        .col.text-center
            input#search(type='search' placeholder='Search for User' onkeyup='searchUsers()')
    br
    //- Finish working on filter
    .container(style='overflow-x:auto')
        table(width='50%').table.table-striped.styleTable
            tr
                th First Name
                th Last Name    
                th Username
                th Password
                th Checking Account
                th Savings Account
                th Email
                th Address
                th City
                th State
                th Zip    
            each acct in newAcct
                tr(id='listOfUsers')
                    td.searches #{acct.firstName}
                    td.searches #{acct.lastName}
                    td #{acct.usName}
                    td #{acct.usPass}
                    td #{acct.checking}
                    td #{acct.savings}
                    td #{acct.email}
                    td #{acct.address}
                    td #{acct.city}
                    td #{acct.state}
                    td #{acct.zip}   
    .row
        .col.text-right
            a(href='/adminWelcome') Back                  